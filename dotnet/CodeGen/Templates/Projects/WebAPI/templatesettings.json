{
  "StartupProjectPath": "CodeGenOutput.API\\",
  "RecreateDatabaseAfterGenerate": true,
  "TestProjectAfterGenerate": true,
  "ConfigBasedGenerator": [
    {
      "T4Template": "CodeGenOutput.API\\DAL\\ApplicationDbContext.tt",
      "Output": "ApplicationDbContext.cs"
    }
  ],
  "ModelsBasedGenerator": [
    {
      "T4Template": "CodeGenOutput.API\\Models\\Model.tt",
      "Output": "{0}.cs"
    },
    {
      "T4Template": "CodeGenOutput.API\\ViewModels\\ViewModel.tt",
      "Output": "{0}VM.cs"
    },
    {
      "T4Template": "CodeGenOutput.API\\Controllers\\Controller.tt",
      "Output": "{0}sController.cs"
    }
  ]
}
